TOOLCHAIN_PATH = $($(TOOLCHAIN_ID)_PATH)
TOOLCHAIN_BIN  = $(TOOLCHAIN_PATH)/bin
KERNEL_PATH    = $($(KERNEL_ID)_PATH)
BUSYBOX_PATH   = $($(BUSYBOX_ID)_PATH)

TOOLCHAIN_ENV  = PATH="$(TOOLCHAIN_BIN):$$PATH"

KERNEL_DTS     = $(KERNEL_PATH)/arch/microblaze/boot/dts
KERNEL_NAME   ?= kernel

MBSL_IMAGE     = $(MBSL_HOME)/image-$(KERNEL_NAME)
